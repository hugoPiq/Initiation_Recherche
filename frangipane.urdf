<?xml version="1.0"?>
<robot name="my_robot">

  <material name="galette">
    <color rgba="0.85 0.66 0.14 1" />
  </material>

  <material name="black">
    <color rgba="0 0 0 1" />
  </material>

  <material name="anthracite">
    <color rgba="0.2 0.2 0.2 1" />
  </material>

  <gazebo reference="wheel_left_link">
    <mu1>1.2</mu1>
    <mu2>1.2</mu2>
    <kp>500000.0</kp>
    <kd>10.0</kd>
    <minDepth>0.001</minDepth>
    <maxVel>0.1</maxVel>
    <material>Gazebo/FlatBlack</material>
  </gazebo>

  <gazebo reference="wheel_right_link">
    <mu1>1.2</mu1>
    <mu2>1.2</mu2>
    <kp>500000.0</kp>
    <kd>10.0</kd>
    <minDepth>0.001</minDepth>
    <maxVel>0.1</maxVel>
    <material>Gazebo/FlatBlack</material>
  </gazebo>

  <gazebo reference="wheel_free_link">
    <mu1>0.01</mu1>
    <mu2>0.01</mu2>
    <!-- <kp>500000.0</kp>
    <kd>10.0</kd>
    <minDepth>0.001</minDepth>
    <maxVel>0.1</maxVel> -->
    <material>Gazebo/FlatBlack</material>
  </gazebo>

  <gazebo reference="frangipane">
    <visual>
      <material name="galette">
        <diffuse>0.85 0.66 0.14 1</diffuse>
        <ambient>0.85 0.66 0.14 1</ambient>
        <emissive>0.3 0.3 0.3 0.5</emissive>
      </material>
    </visual>
  </gazebo>


  <link name="base_footprint" />

  <joint name="base_joint" type="fixed">
    <parent link="base_footprint" />
    <child link="frangipane" />
    <origin xyz="0 0 0.1" rpy="0 0 0" />
  </joint>


  <link name="frangipane">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <cylinder length="0.1" radius="0.4" />
      </geometry>
      <material name="galette" />
    </visual>

    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <cylinder length="0.1" radius="0.4" />
      </geometry>
    </collision>

    <inertial>
      <origin xyz="0 0 0" />
      <mass value="0.5" />
      <inertia ixx="0.04083333333333334" ixy="0.0" ixz="0.0" iyy="0.04083333333333334" iyz="0.0" izz="0.08000000000000002" />
    </inertial>

  </link>


  <joint name="essieu_joint" type="fixed">
    <parent link="frangipane" />
    <child link="essieu" />
    <origin xyz="-0.2 0 -0.04" rpy="1.57 0 0" />
  </joint>

  <link name="essieu">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <cylinder length="0.76" radius="0.02" />
      </geometry>
      <material name="galette" />
    </visual>

    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <cylinder length="0.76" radius="0.02" />
      </geometry>
    </collision>

    <inertial>
      <origin xyz="0 0 0" />
      <mass value="0.2" />
      <inertia ixx="0.04823333333333333" ixy="0.0" ixz="0.0" iyy="0.04823333333333333" iyz="0.0" izz="0.0002" />
    </inertial>
  </link>


  <joint name="wheel_left_joint" type="continuous">
    <parent link="frangipane" />
    <child link="wheel_left_link" />
    <origin xyz="-0.2 0.41 -0.04" rpy="-1.57 0 0" />
    <axis xyz="0 0 1" />
  </joint>

  <link name="wheel_left_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <cylinder length="0.04" radius="0.06" />
      </geometry>
      <material name="anthracite" />
    </visual>

    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <cylinder length="0.04" radius="0.06" />
      </geometry>
    </collision>

    <inertial>
      <origin xyz="0 0 0" />
      <mass value="0.1" />
      <inertia ixx="0.0010333333333333334" ixy="0.0" ixz="0.0" iyy="0.0010333333333333334" iyz="0.0" izz="0.0018" />
    </inertial>
  </link>


  <joint name="wheel_right_joint" type="continuous">
    <parent link="frangipane" />
    <child link="wheel_right_link" />
    <origin xyz="-0.2 -0.41 -0.04" rpy="-1.57 0 0" />
    <axis xyz="0 0 1" />
  </joint>

  <link name="wheel_right_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <cylinder length="0.04" radius="0.06" />
      </geometry>
      <material name="anthracite" />
    </visual>

    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <cylinder length="0.04" radius="0.06" />
      </geometry>
    </collision>

    <inertial>
      <origin xyz="0 0 0" />
      <mass value="0.1" />
      <inertia ixx="0.0010333333333333334" ixy="0.0" ixz="0.0" iyy="0.0010333333333333334" iyz="0.0" izz="0.0018" />
    </inertial>
  </link>


  <joint name="wheel_free_joint" type="fixed">
    <parent link="frangipane" />
    <child link="wheel_free_link" />
    <origin xyz="0.2 0 -0.05" rpy="-1.57 0 0" />
  </joint>

  <link name="wheel_free_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <sphere radius="0.05" />
      </geometry>
      <material name="anthracite" />
    </visual>

    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <sphere radius="0.05" />
      </geometry>
    </collision>

    <inertial>
      <origin xyz="0 0 0" />
      <mass value="0.05" />
      <inertia ixx="5.0000000000000016e-05" ixy="0.0" ixz="0.0" iyy="5.0000000000000016e-05" iyz="0.0" izz="5.0000000000000016e-05" />
    </inertial>
  </link>

  <gazebo>
    <plugin name="joint_states" filename="libgazebo_ros_joint_state_publisher.so">
      <joint_name>wheel_left_joint</joint_name>
      <joint_name>wheel_right_joint</joint_name>
    </plugin>

    <plugin name='diff_drive' filename='libgazebo_ros_diff_drive.so'>

      <!-- wheels -->
      <left_joint>wheel_left_joint</left_joint>
      <right_joint>wheel_right_joint</right_joint>

      <!-- kinematics -->
      <wheel_separation>0.4</wheel_separation>
      <wheel_diameter>0.06</wheel_diameter>

      <!-- limits -->
      <max_wheel_torque>20</max_wheel_torque>
      <max_wheel_acceleration>1.0</max_wheel_acceleration>

      <!-- output -->
      <publish_odom>true</publish_odom>
      <publish_tf>true</publish_tf> <!-- publish_odom_tf-->
      <publish_wheel_tf>true</publish_wheel_tf>

      <robot_base_frame>frangipane</robot_base_frame>

    </plugin>
  </gazebo>


</robot> <!-- <ros>
  <namespace>/cam</namespace>
  <remapping>~/image_raw:=custom_img</remapping>
  <remapping>~/camera_info:=custom_info</remapping>
</ros> -->